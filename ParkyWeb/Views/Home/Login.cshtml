@model ParkyWeb.Models.User
@{
    ViewData["Title"] = "Login";
}

<h1 class="mb-3">Login</h1>
<form method="post">
<div class="row mb-5">
    <div class="col-3">
    <input asp-for="Username" type="text" placeholder="Username ..." class="form-control"/>
  </div>
  <div class="col-3">
    <input asp-for="Password" type="password" placeholder="Password ..." class="form-control"/>
  </div>
    <div class="col-3">
    <button type="submit" value="Submit" class="btn btn-success form-control" style="width:100px;">Login</button>
  </div>
    </div>
</form>

