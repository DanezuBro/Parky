@model ParkyWeb.Models.NationalPark

<div class="container p-4" style="border-radius:20px;">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">National Park List</h2>
        </div>

        <div class="col-6 text-end text-white">
            <a asp-action="Upsert" asp-controller="NationalParks" class="btn btn-primary"><i class="fa-solid fa-plus"></i> Create New National Park</a>
        </div>
    </div>

    <br />
    <div style="background-color:#cde8e2" class="p-3 rounded">
        <table id="tblData" class="table table-bordered table-striped" style="width:100%;">
            <thead>
                <tr class="table-primary">
                    <th scope="col" class="text-center">Name</th>
                    <th scope="col" class="text-center">State</th>
                    <th scope="col" class="text-center">Actions</th>
                </tr>
            </thead>
        </table>
    </div>



</div>

@section Scripts{
<script src="~/js/nationalPark.js"></script>
}